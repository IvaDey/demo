{
  "name": "amp-backend",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "build": "npm run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "tsnd -r tsconfig-paths/register --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "lint": "eslint  --ext ts,tsx .",
    "check": "tsc --noEmit && npm run lint",
    "lint:fix": "eslint  --ext ts,tsx --fix .",
    "start:db": "docker-compose -f docker-compose.dev-db.yml up",
    "generate-todo-md": "./bin/generate-todo-md/todo.md.sh -e README.md"
  },
  "dependencies": {
    "@tsed/ajv": "^7.67.5",
    "@tsed/common": "^7.67.5",
    "@tsed/core": "^7.67.5",
    "@tsed/di": "^7.67.5",
    "@tsed/engines": "^7.67.5",
    "@tsed/exceptions": "^7.67.5",
    "@tsed/json-mapper": "^7.67.5",
    "@tsed/logger": "^6.7.5",
    "@tsed/logger-file": "^6.7.5",
    "@tsed/objection": "^7.67.5",
    "@tsed/openspec": "^7.67.5",
    "@tsed/platform-cache": "^7.67.5",
    "@tsed/platform-exceptions": "^7.67.5",
    "@tsed/platform-express": "^7.67.5",
    "@tsed/platform-log-middleware": "^7.67.5",
    "@tsed/platform-middlewares": "^7.67.5",
    "@tsed/platform-params": "^7.67.5",
    "@tsed/platform-response-filter": "^7.67.5",
    "@tsed/platform-views": "^7.67.5",
    "@tsed/schema": "^7.67.5",
    "@tsed/swagger": "^7.67.5",
    "ajv": "^8.12.0",
    "barrelsby": "^2.8.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "dotenv-flow": "^4.1.0",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "method-override": "^3.0.0",
    "objection": "^3.1.4",
    "pg": "^8.11.5"
  },
  "devDependencies": {
    "@tsed/eslint-plugin": "^1.1.11",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/method-override": "^0.0.35",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.1",
    "ts-node": "^10.9.2",
